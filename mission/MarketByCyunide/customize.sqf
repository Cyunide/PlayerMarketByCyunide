// Max Listing Setting
// You can set this to the max amount of listings that can be available at any time.
// Set to -1 to disable listing limit
// Default: -1
CyPM_MAX_LISTINGS = -1;			

// Max Listings Per User Setting
// You can set this to the max amount of listings a user can create.
// Set to -1 to disable limit
// Default: -1
CyPM_MAX_LISTINGS_USER = -1;

// Max Sell Amount Setting
// You can set this to the max amount an item can be sold for.
// Set to -1 to disable limit
// Default: -1
CyPM_MAX_SELL_AMOUNT = -1;

// Min Sell Amount Setting
// You can set this to the minimum amount an item can be sold for.
// Set to -1 to disable limit
// Default: -1
CyPM_MIN_SELL_AMOUNT = 17;